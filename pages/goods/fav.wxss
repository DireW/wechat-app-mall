page {
    background: #f5f5f5;
}

page, view, image, input {
    display: block;
    box-sizing: border-box;
}

.ellipsis {
    display: -webkit-box;
    -webkit-line-clamp: 2;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
    word-break: break-all;
}

.list2-box {
    margin: 16 rpx;
    width: 718 rpx;
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
}

.list2 {
    margin: 16 rpx 0;
    width: 350 rpx;
    background: #fff;
    border-radius: 30 rpx;
    overflow: hidden;
}

.list2 .img {
    width: 350 rpx;
    height: 350 rpx;
}

.list2 .goods-info {
    padding: 16 rpx;
}

.list2 .goods-info .title {
    color: #333;
    font-size: 26 rpx;
}

.list2 .goods-info .price {
    color: #FF3B30;
    font-size: 34 rpx;
}

.delete-btn {
    width: 55 rpx;
    height: 55 rpx;
    margin: auto;
    margin-top: 32 rpx;
}
